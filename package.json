{
  "name": "avataaars",
  "version": "2.0.0",
  "description": "React component for avataaars",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc"
  },
  "keywords": [
    "react",
    "avatar"
  ],
  "repository": "git@github.com:fangpenlin/avataaars.git",
  "author": "Fang-Pen Lin <hello@fangpenlin.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/react": "^19.1.3",
    "eslint": "^9.26.0",
    "import-sort-style-renke": "^6.0.0",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "tslint-react": "^5.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "dependencies": {
    "eslint-plugin-react": "^7.37.5",
    "lodash": ">=4.17.21",
    "prop-types": "^15.8.1"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "import-sort-style-renke"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "import-sort-style-renke"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": true
  }
}