{
  "name": "avataaars-kegenguyll",
  "version": "1.0.0",
  "description": "React component for avataaars",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts,.tsx --fix --ignore-pattern dist",
    "release": "standard-version"
  },
  "keywords": [
    "react",
    "avatar"
  ],
  "repository": "git@github.com:KegenGuyll/avataaars.git",
  "author": "KegenGuyll",
  "license": "MIT",
  "peerDependencies": {
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/lodash": "^4.17.16",
    "@types/react": "^19.1.3",
    "eslint": "^9.26.0",
    "import-sort-style-renke": "^6.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "standard-version": "^9.5.0"
  },
  "dependencies": {
    "eslint-plugin-react": "^7.37.5",
    "lodash": ">=4.17.21",
    "prop-types": "^15.8.1",
    "react-dom": "^19.1.0"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "import-sort-style-renke"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "import-sort-style-renke"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": true
  },
  "files": [
    "dist"
  ]
}